<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.englishapp.Login">
   <children>
      <TextField fx:id="usernameTextField" layoutX="259.0" layoutY="163.0" />
      <PasswordField fx:id="passwordTextField" layoutX="259.0" layoutY="225.0" />
      <Label layoutX="124.0" layoutY="167.0" prefHeight="20.0" prefWidth="87.0" text="Username">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Label layoutX="130.0" layoutY="224.0" text="Password">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="loginButton" layoutX="272.0" layoutY="279.0" mnemonicParsing="false" onAction="#login" text="Login">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label fx:id="wrongPasswordLabel" alignment="CENTER" layoutX="-1.0" layoutY="106.0" prefHeight="17.0" prefWidth="600.0">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Button layoutX="326.0" layoutY="337.0" mnemonicParsing="false" onAction="#signup" text="Sign up" />
      <Label layoutX="168.0" layoutY="341.0" text="Do not have an account?">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
